  "author": "Meet5555",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Meet5555/project-creator.git"
  },
  "bugs": {
    "url": "https://github.com/Meet5555/project-creator/issues"
  },
  "homepage": "https://github.com/Meet5555/project-creator#readme",
