    "dev:next": "node bin/cli.js next",
    "start": "node bin/cli.js",
    "prepublishOnly": "npm run test"
  },
  "keywords": [
    "react",
    "nextjs",
    "project-structure",
    "scaffold",
    "generator",
    "create-react-app",
